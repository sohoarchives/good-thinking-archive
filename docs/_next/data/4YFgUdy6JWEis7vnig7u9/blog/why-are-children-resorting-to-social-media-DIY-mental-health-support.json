		"blogData": {
			"title": "Why are so many children resorting to ‘social media DIY’ mental health support?",
			"slug": "why-are-children-resorting-to-social-media-DIY-mental-health-support",
			"subtitle": "In this guest blog, Dr Nihara Krause MBE, Consultant Clinical Psychologist and CEO and founder of stem4, discusses the charity’s latest survey findings. ",
			"updatedAt": "2023-11-07T11:48:21.894Z",
			"metadata": {
				"id": "768",
				"description": "Dr Nihara Krause MBE, Consultant Clinical Psychologist and CEO and founder of stem4, discusses the charity’s latest survey findings.",
				"title": "Why are children resorting to ‘social media’ mental health support?",
				"image": { "data": null }
			},
			"author": {
				"id": "36",
				"name": "Dr Nihara Krause MBE",
				"title": "Consultant Clinical Psychologist and CEO and founder of stem4",
				"headshot": {
					"data": {
						"id": "543",
						"attributes": {
							"alternativeText": "Picture1.png",
							"width": 314,
							"height": 280,
							"mime": "image/png",
							"url": "/good-thinking-archive/assets/Picture1_ec64d5fd03.png",
							"size": 32.79,
							"formats": {
								"thumbnail": {
									"ext": ".png",
									"url": "/good-thinking-archive/assets/thumbnail_Picture1_ec64d5fd03.png",
									"hash": "thumbnail_Picture1_ec64d5fd03",
									"mime": "image/png",
									"name": "thumbnail_Picture1.png",
									"path": null,
									"size": 45.19,
									"width": 175,
									"height": 156
								}
							},
							"hash": "Picture1_ec64d5fd03",
							"name": "Picture1.png",
